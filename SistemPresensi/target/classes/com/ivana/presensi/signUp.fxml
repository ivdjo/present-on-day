<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="signUp" prefHeight="642.0" prefWidth="749.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ivana.presensi.SignUpController">
   <children>
      <TextField fx:id="txtUsername" layoutX="192.0" layoutY="72.0" prefHeight="51.0" prefWidth="354.0" promptText="Username">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <TextField fx:id="txtName" layoutX="192.0" layoutY="245.0" prefHeight="51.0" prefWidth="354.0" promptText="Full Name">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="192.0" layoutY="159.0" prefHeight="51.0" prefWidth="354.0" promptText="Password">
         <font>
            <Font size="24.0" />
         </font>
      </PasswordField>
      <RadioButton fx:id="rbBoy" layoutX="217.0" layoutY="332.0" mnemonicParsing="false" text="Pria">
         <font>
            <Font size="24.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="tgGender" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rbGril" layoutX="369.0" layoutY="332.0" mnemonicParsing="false" text="Wanita" toggleGroup="$tgGender">
         <font>
            <Font size="24.0" />
         </font>
      </RadioButton>
      <ComboBox fx:id="comboLevel" layoutX="192.0" layoutY="416.0" prefHeight="44.0" prefWidth="354.0" promptText="Select Level" />
      <Button layoutX="565.0" layoutY="577.0" mnemonicParsing="false" onAction="#btnSubmit" prefHeight="51.0" prefWidth="170.0" text="Submit">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="577.0" mnemonicParsing="false" onAction="#btnLogin" prefHeight="51.0" prefWidth="170.0" text="Login">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
